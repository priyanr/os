OUTPUT_FORMAT("elf32-littlearm")
phys = 0xf001d000;
SECTIONS
{
	. = 0xf0010000;
	.entry : {
	*(.entry)
	}
	  . = 0xf001c000;
	.trampoline : {
	*(.trampoline)
	}
	
 	.text phys : AT(phys) {
 		code = .;
 		*(.text)
 		*(.rodata)
 		. = ALIGN(4096);
 	}
	.data : AT(phys + (data - code))
	{
		data = .;
		*(.data)
		. = ALIGN(4096);
	}
	__bss_start__ = .;
	.bss : AT(phys + (bss - code))
	{
		bss = .;
		*(.bss)
		. = ALIGN(4096);
	}
	__bss_end__ = .;
	.note.gnu.build-id : AT(phys + (note_gnu_build_id - code))
	{
		note_gnu_build_id = .;
		*(.note.gnu.build-id)
		. = ALIGN(4096);
	}
	__end__ = .;
}
